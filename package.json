{
  "name": "angular-jaba",
  "version": "2.2.0",
  "description": "Projeto para testes com angular",
  "main": "src/app.js",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.production.js --progress --bail",
    "clean": "rimraf dist/*",
    "build-server": "webpack-dev-server --config webpack.config.production.js --progress --colors --inline --content-base app/ --hot --history-api-fallback --compress",
    "test": "karma start",
    "dev": "webpack-dev-server --config webpack.config.dev.js --output-pathinfo --progress --colors --inline --content-base app/ --hot --history-api-fallback",
    "stats": "webpack --config webpack.config.production.js --progress --profile --json > stats.json",
    "comp": "npm rebuild node-sass"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauricionofre/Angular-Jaba"
  },
  "keywords": [
    "Seed",
    "Jabacule",
    "Startup"
  ],
  "author": "Mauricio Onofre",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-i18n": "1.6.4",
    "angular-loading-bar": "0.9.0",
    "angular-sanitize": "1.6.4",
    "angular-translate": "2.15.1",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-grid": "4.0.4",
    "angular-ui-router": "0.2.15",
    "bootstrap": "3.3.6",
    "bootstrap-sass": "3.3.7",
    "eonasdan-bootstrap-datetimepicker": "4.17.47",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "lodash.debounce": "4.0.8",
    "oclazyload": "1.1.0",
    "webpack2-polyfill-plugin": "0.0.2"
  },
  "devDependencies": {
    "angular-mocks": "1.6.4",
    "css-loader": "0.28.0",
    "ejs-loader": "0.3.0",
    "expose-loader": "0.7.3",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "import-glob-loader": "1.1.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "ng-annotate-loader": "0.2.0",
    "node-sass": "4.5.2",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "strict-loader": "1.2.0",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  }
}
